void mata(int xx, int yy, int i){
  display.clearDisplay();
  display.invertDisplay(true);
  display.fillCircle(xx, yy, 20, WHITE);
  switch(i){
    case 1:
      //name = "kedip1";
      display.drawBitmap(0,0,kedip1, 128, 64, 1);
      break;
    case 2:
      display.drawBitmap(0,0,kedip2, 128, 64, 1);
      break;
    case 3:
      display.drawBitmap(0,0,kedip3, 128, 64, 1);
      break;
    case 4:
      display.drawBitmap(0,0,kedip4, 128, 64, 1);
      break;
    default:
      display.drawBitmap(0,0,eye2, 128, 64, 1);
      break;
  }
  display.display();
}

void kedip(int xx, int yy){
  mata(xx,yy,1);
  //delay(1);
  mata(xx,yy,2);
  //delay(1);
  mata(xx,yy,3);
  //delay(1);
  mata(xx,yy,4);
