void setup() {
  Serial.begin(9600);
  if(!display.begin(SSD1306_SWITCHCAPVCC, 0x3C)) {
    Serial.println(F("SSD1306 allocation failed"));
    for(;;);
  }
}

void loop() {
  t += 1;
  if(Serial.available() > 0)
  {
    if(Serial.read() == 'X')
    {
      x = Serial.parseInt();
      if(Serial.read() == 'Y')
      {
       y = Serial.parseInt();
       //mata(x,y,5);
       if (t > 99){
          kedip(x,y);
          t = 0;
       }
       else if (t < 99){
        mata(x,y,5);
       }
      }
    }
    //while(Serial.available() > 0)
    //{
      //Serial.read();
    //}
  }
  else if(Serial.available() == 0){
    //Pos(64,32);
    if (t > 39){
      kedip(64,32);
      t = 0;
    }
    else if (t < 39){
      mata(64,32,5);
    }
  }
  delay(10);
  //Serial.println(t);
}
